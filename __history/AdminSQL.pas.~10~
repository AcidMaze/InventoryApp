    { Public declarations }
  end;

var
  Form19: TForm19;

implementation
uses DM, AdminPanel;

{$R *.dfm}

procedure TForm19.Button1Click(Sender: TObject);
begin
  if Length(Memo1.Text) = 0 then
  begin
    ShowMessage('ќшибка! ¬ведите новое название кабинета.');
    exit;
  end;

  with DM.DataModule2.User_Query do
  begin
    Active := false;
    SQL.Clear;
    SQL.Text := ;
    Active:= true;
  end;
end;

procedure TForm19.Button2Click(Sender: TObject);
begin
  Form19.Close;
end;

procedure TForm19.FormActivate(Sender: TObject);
begin
  Memo1.Cler;
end;

procedure TForm19.FormClose(Sender: TObject; var Action: TCloseAction);
begin
  Form11.Enabled := true;
end;

end.
